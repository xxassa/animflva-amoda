{
    "name": "AnimeFLV",
    "version": "1.0.0",
    "author": {
        "name": "sobet",
        "website": "https://google.com"
    },
    "iconURL": "https://upload.wikimedia.org/wikipedia/commons/thumb/7/7f/Animeflv_logo.svg/512px-Animeflv_logo.svg.png",
    "stream": "MP4",
    "language": "ES",
    "extractor": "normal",
    "module": [
        {
            "search": {
                "url": "https://www3.animeflv.net/browse",
                "parameter": "q",
                "documentSelector": ".ListAnimes .ListItem",
                "title": ".Title",
                "image": {
                    "url": ".Image img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "featured": {
                "url": "https://www3.animeflv.net",
                "documentSelector": ".ListAnimes .ListItem",
                "title": ".Title",
                "image": {
                    "url": ".Image img",
                    "attribute": "src"
                },
                "href": "a"
            },
            "details": {
                "baseURL": "https://www3.animeflv.net",
                "aliases": {
                    "selector": ".Ficha .Container .Title",
                    "attribute": "text"
                },
                "synopsis": ".Description",
                "airdate": ".Container .Date",
                "stars": ".fa-star"
            },
            "episodes": {
                "selector": ".Container .ListCaps a",
                "order": "ordered",
                "pattern": ""
            }
        }
    ]
}
